{
    "comments": false,
    "env": {
        "main": {
            "presets": [
                ["env", {
                    "targets": { "node": 12 }
                }],
                "stage-2"
            ]
        },
        "renderer": {
            "presets": [
                ["env", {
                    "modules": false
                }],
                "stage-2"
            ]
        },
        "web": {
            "presets": [
                ["env", {
                    "modules": false
                }],
                "stage-2"
            ]
        }
    },
    "plugins": ["transform-runtime", "transform-es2015-modules-commonjs"]
}